{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ContractSettings": {
    "Contracts": [
      {
        "Type": "Employment",
        "TemplatePath": "C:\\Contracts\\Templates\\EmploymentTemplate.docx",
        "Placeholders": {
          "{EmployeeName}": "ClientName",
          "{StartDate}": "StartDate",
          "{EndDate}": "EndDate",
          "{Salary}": "Amount"
        }
      },
      {
        "Type": "Service",
        "TemplatePath": "C:\\Contracts\\Templates\\ServiceTemplate.docx",
        "Placeholders": {
          "{ClientName}": "ClientName",
          "{ServiceStart}": "StartDate",
          "{ServiceEnd}": "EndDate",
          "{Fee}": "Amount"
        }
      }
    ]
  }


}
